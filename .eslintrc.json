{
    "env": {
        "browser": true,
        "es2021": true,
        "react-native/react-native": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "airbnb/hooks",
        "universe/native"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "react-hooks/exhaustive-deps": 1,
        "react/react-in-jsx-scope": 0,
        "react/style-prop-object": 0,
        "react/no-unstable-nested-components": 0,
        "react/jsx-fragments": 0,
        "react/prop-types": 0,
        "no-use-before-define": 0,
        "no-unused-vars": 2,
        "func-names": 0,
        "global-require": 0
    }
}
